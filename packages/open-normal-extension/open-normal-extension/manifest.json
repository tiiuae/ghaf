{
  "manifest_version": 3,
  "name": "Open in normal browser",
  "version": "VERSION_HERE",
  "permissions": ["nativeMessaging", "contextMenus", "activeTab"],
  "icons": {
    "16": "ghaf-logo-512px.png",
    "32": "ghaf-logo-512px.png",
    "48": "ghaf-logo-512px.png",
    "128": "ghaf-logo-512px.png"
  },
  "background": {
    "service_worker": "open_normal.js"
  },
  "action": {
    "default_title": "Open page in normal browser"
  },
  "update_url": "http://localhost:8080/update.xml"
}
