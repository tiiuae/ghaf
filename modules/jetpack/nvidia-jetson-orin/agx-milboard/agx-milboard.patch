+						endpoint@0 {
+							status = "okay";
+							port-index = <0>;
+							bus-width = <2>;
+							remote-endpoint = <&rbpcv3_imx477_out0>;
+						};
+					};
+					port@1 {
+						endpoint@1 {
+							status = "okay";
+						};
+					};
+				};
+			};
+			channel@1 {
+				ports {
+					port@2 {
+						endpoint@2 {
+							status = "okay";
+							port-index = <2>;
+							bus-width = <2>;
+							remote-endpoint = <&rbpcv3_imx477_out1>;
+						};
+					};
+					port@3 {
+						endpoint@3 {
+							status = "okay";
+						};
+					};
+				};
+			};
+		};
+	};
